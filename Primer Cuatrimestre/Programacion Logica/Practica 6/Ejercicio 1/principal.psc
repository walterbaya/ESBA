Funcion area <- areaRectangulo(base, altura)
	area <- base * altura
Fin Funcion

Algoritmo principal
	Escribir "Ingrese la base del rectangulo: "
	Leer base
	Escribir "Ingrese la altura del rectangulo: "
	Leer altura
	area <- areaRectangulo(base, altura)
	Escribir "Mostrar el area del rectangulo: ", area
FinAlgoritmo

